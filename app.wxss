/**app.wxss**/

.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  font-size: 28rpx;
} 

.content_box{
  flex: 1;
  background-color: #fff;
}

.clearfix:after{/*伪元素是行内元素 清除浮动方法*/
  content: "";
  display: block;
  height: 0;
  clear:both;
  visibility: hidden;
}


.padding_lr{
  padding: 0 20rpx;
}

.padding_tb{
  padding: 20rpx 0;
}

.padding_trbl{
  padding: 20rpx 20rpx;
}

.padding_l{
  padding-left: 20rpx;
}
.padding_r{
  padding-left: 20rpx;
}


