


<view class="item_wrapper" wx:for="{{list}}" wx:key="*this">
  <view
  bindtap="onNavigateTo"
  hover-class="none" 
  class="label_wrap" 
  wx:for="{{item}}" 
  wx:for-item="itm" 
  wx:key="*this"> 
    
    <icon class="iconfont {{itm.icon}}"></icon>

    <view class="label">{{itm.title}}</view>
  </view>
</view>
