  margin-left: 29rpx;
  display: flex;
  flex-flow: column;
  justify-content: space-between;
  /* width: calc(100vw - 210rpx); */
  height: 285.96rpx;
  
}
.ticketDetail .header_wrapper .header_content .header_detail .header_detail_right .title{
  width: 100%;
  font-size: 35rpx;
}
.ticketDetail .header_wrapper .header_content .header_detail .header_detail_right .price_scope{
