  width: 30vw;
  text-align: right;
}



.ticketDetail .header_wrapper .header_content .header .header_right_icon .margin_right{
  margin-right: 20rpx;
}
.ticketDetail .header_wrapper .img_filter_blur{
  width: 100%;
  height: 100%;
  background-color: #b8abab;
  filter: blur(30rpx);
}

.ticketDetail .header_wrapper .header_content .header_detail{
  display: flex;
