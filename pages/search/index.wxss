.search_container .no_content{
  height: 300rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-flow: column;
}
.search_container .no_content .relative_content{
  color: #e0e0e0;
  font-size: 25rpx;
}


.hot_history_search .history_wrapper{
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 50rpx;
}
.history_content{
  margin-top: 20rpx;
}
.history_content .history_item{
  display: inline-block;
  padding: 20rpx 30rpx;
  background: #f5f5f5;
  margin:0 15rpx 20rpx 0;
  border-radius: 10rpx;
}