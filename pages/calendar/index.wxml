<view class="container">

    <view class="calendar_header padding_lr">
        <text></text>
        演出日历
        <van-icon name="ellipsis" size="25"/>
    </view>

    <my-tab bindonSelectLocation="onSelectLocation" bindgetCategoryId="getCategoryId"/>

    <scroll-view 
    scroll-y="{{true}}" 
    class="scroll_wrapper" 
    upper-threshold="50" 
    lower-threshold="50" 
    bindscrolltoupper="bindscrolltoupper" 
    bindscrolltolower="bindscrolltolower">
        <view class="calendar_wrapper">
            <my-calendar
            bind:afterTapDay="afterTapDay"
            calendarConfig="{{calendarConfig}}">
            
            </my-calendar>
        </view>

        <view class="content_list padding_lr">
            <view class="list_item">
                
            </view>
        </view>

        

    

    </scroll-view>





</view>
